local pcFlyBtn = Instance.new("TextButton", visPage)
pcFlyBtn.Size = UDim2.new(0.4,0,0,40)
pcFlyBtn.Position = UDim2.new(0.05,0,0.1,0)
pcFlyBtn.Text = "PC Fly: Off"
pcFlyBtn.BackgroundColor3 = RED
pcFlyBtn.TextColor3 = Color3.new(1,1,1)

local mobileFlyBtn = Instance.new("TextButton", visPage)
mobileFlyBtn.Size = UDim2.new(0.4,0,0,40)
mobileFlyBtn.Position = UDim2.new(0.55,0,0.1,0)
mobileFlyBtn.Text = "Mobile Fly: Off"
mobileFlyBtn.BackgroundColor3 = RED
mobileFlyBtn.TextColor3 = Color3.new(1,1,1)

local pcFly = false
local mobileFly = false
local flySpeed = 50
local bodyVelocity

local function stopFly()
    pcFly = false
    mobileFly = false
    RunService:UnbindFromRenderStep("Fly")
    if bodyVelocity then bodyVelocity:Destroy() bodyVelocity = nil end
    pcFlyBtn.Text = "PC Fly: Off"
    mobileFlyBtn.Text = "Mobile Fly: Off"
end

local function startFly(mode)
    stopFly()
    local root = Character:FindFirstChild("HumanoidRootPart")
    if not root then return end

    bodyVelocity = Instance.new("BodyVelocity")
    bodyVelocity.MaxForce = Vector3.new(1e5,1e5,1e5)
    bodyVelocity.Parent = root

    if mode == "PC" then
        pcFly = true
        pcFlyBtn.Text = "PC Fly: On"
        RunService:BindToRenderStep("Fly",301,function()
            local cam = workspace.CurrentCamera
            local dir = Vector3.zero
            if UserInputService:IsKeyDown(Enum.KeyCode.W) then dir += cam.CFrame.LookVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.S) then dir -= cam.CFrame.LookVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.A) then dir -= cam.CFrame.RightVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.D) then dir += cam.CFrame.RightVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.Space) then dir += Vector3.new(0,1,0) end
            if UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then dir -= Vector3.new(0,1,0) end
            bodyVelocity.Velocity = dir.Magnitude>0 and dir.Unit*flySpeed or Vector3.zero
        end)
    elseif mode == "Mobile" then
        mobileFly = true
        mobileFlyBtn.Text = "Mobile Fly: On"
        RunService:BindToRenderStep("Fly",301,function()
            local dir = Vector3.new(0,0,1)
            bodyVelocity.Velocity = dir*flySpeed
        end)
    end
end

pcFlyBtn.MouseButton1Click:Connect(function()
    if pcFly then stopFly() else startFly("PC") end
end)

mobileFlyBtn.MouseButton1Click:Connect(function()
    if mobileFly then stopFly() else startFly("Mobile") end
end)

-- ===========================
-- BUTTONS LOGIC
-- ===========================
closeBtn.MouseButton1Click:Connect(function()
    gui:Destroy()
end)

minBtn.MouseButton1Click:Connect(function()
    frame.Visible = false
    restoreBtn.Visible = true
end)

restoreBtn.MouseButton1Click:Connect(function()
    frame.Visible = true
    restoreBtn.Visible = false
end)

-- ===========================
-- KEY LOGIC
-- ===========================
keyBtn.MouseButton1Click:Connect(function()
    if keyBox.Text == CORRECT_KEY then
        keyGui:Destroy()
        gui.Enabled = true
    else
        keyMsg.Text = "Wrong Key"
        keyMsg.TextColor3 = RED
    end
end)

-- ===========================
-- FPS UPDATE
-- ===========================
local frames = 0
local last = tick()
RunService.RenderStepped:Connect(function()
    frames += 1
    if tick() - last >= 1 then
        fpsLabel.Text = "FPS: "..frames
        frames = 0
        last = tick()
    end
end)
