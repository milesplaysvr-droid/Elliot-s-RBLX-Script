--MMod ON TOP
local Players = game:GetService("Players")
local CoreGui = game:GetService("CoreGui")
local TweenService = game:GetService("TweenService")

-- Key verification
local correctKey = "Rockyonion123?"

-- Theme Colors
local COLOR_BLACK = Color3.fromRGB(0, 0, 0)
local COLOR_YELLOW = Color3.fromRGB(255, 255, 0)
local COLOR_DARK_RED = Color3.fromRGB(150, 0, 0)
local COLOR_BRIGHT_RED = Color3.fromRGB(228, 0, 0)
local COLOR_DARK_GREY = Color3.fromRGB(25, 25, 25)
local COLOR_GREEN = Color3.fromRGB(0, 150, 0)

-- ===========================
-- 1. LOADING SCREEN
-- ===========================
local loadingGui = Instance.new("ScreenGui")
loadingGui.Name = "LoadingScreen"
loadingGui.Parent = CoreGui

local loadingFrame = Instance.new("Frame")
loadingFrame.Size = UDim2.new(0, 400, 0, 200)
loadingFrame.Position = UDim2.new(0.5, -200, 0.5, -100)
loadingFrame.BackgroundColor3 = COLOR_BLACK
loadingFrame.BorderSizePixel = 0
loadingFrame.Parent = loadingGui

local loadingCorner = Instance.new("UICorner")
loadingCorner.CornerRadius = UDim.new(0, 20)
loadingCorner.Parent = loadingFrame

-- Progress bar background
local progressBG = Instance.new("Frame")
progressBG.Size = UDim2.new(1, 0, 0, 30)
progressBG.Position = UDim2.new(0, 0, 0.4, 0)
progressBG.BackgroundColor3 = Color3.fromRGB(50,50,50)
progressBG.Parent = loadingFrame

local bgCorner = Instance.new("UICorner")
bgCorner.CornerRadius = UDim.new(0,15)
bgCorner.Parent = progressBG

-- Progress bar fill
local progressBar = Instance.new("Frame")
progressBar.Size = UDim2.new(0,0,1,0)
progressBar.BackgroundColor3 = COLOR_YELLOW
progressBar.Parent = progressBG

local fillCorner = Instance.new("UICorner")
fillCorner.CornerRadius = UDim.new(0,15)
fillCorner.Parent = progressBar

-- Copy YouTube button
local copyButton = Instance.new("TextButton")
copyButton.Size = UDim2.new(0.6, 0, 0, 50)
copyButton.Position = UDim2.new(0.2, 0, 0.7, 0)
copyButton.Text = "Copy YouTube Link"
copyButton.BackgroundColor3 = COLOR_GREEN
copyButton.TextColor3 = Color3.new(1,1,1)
copyButton.Font = Enum.Font.SourceSansBold
copyButton.TextScaled = true
copyButton.TextWrapped = true
copyButton.Parent = loadingFrame

-- Squircle + optional outline
local copyCorner = Instance.new("UICorner")
copyCorner.CornerRadius = UDim.new(0,15)
copyCorner.Parent = copyButton

local copyStroke = Instance.new("UIStroke")
copyStroke.Color = COLOR_BLACK
copyStroke.Thickness = 2
copyStroke.Parent = copyButton

-- Copy functionality
copyButton.MouseButton1Click:Connect(function()
    setclipboard("https://www.youtube.com/@Elliot-b2q")
    copyButton.Text = "Copied!"
end)

-- Slow loading animation
for i=0,1,0.002 do
    progressBar.Size = UDim2.new(i,0,1,0)
    wait(0.03)
end

loadingGui:Destroy()

-- ===========================
